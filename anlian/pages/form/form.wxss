/*===== message1-message2 =====*/
.message-text:before {
	content: "";
	display: table;
	clear: both;
}
.message-text .text {
	display: block;
	text-align: right;
	padding: 10px 30px 10px 0;
	color: #666;
	font-size: 14px;
}
.message-text .text .pink {
	color: #f57eb6
;
}
.message input {
	text-align: right;
}
.message .check {
	top: 0;
}
.message .formGroup .small {
	position: relative;
	margin-left: -10px;
	font-size: 12px;
	color: #999;
	font-style: italic;
	white-space: nowrap;
}
.message .checkbox checkbox {
	position: absolute;
	top: 50%;
	right: 15px;
	transform: translate(0, -50%);
}
.btn {
	width: 100px;
    height: 30px;
    font-size: 14px;
    color: #ff2121;
    background: white;
}
.btn text {
	display: inline-block;
	line-height: 33px;
	padding-left: 5px;
	vertical-align: top;
}
.btn.add_btn {
	color: white;
}
.btn.add_btn.orange {
	background: #fe8100;
}
.btn.add_btn.pink {
	background: #f57eb6;
}
.btn.add_btn.green {
	background: #64a70c;
}
.btn.add_btn.yellow {
	background: #f8b300;
}
.btn image {
	width: 15px;
	height: 22px;
	padding-top: 2px;
	vertical-align: top;
}
.mg-bottom {
	margin-bottom: 10px;
}
.w {
	width: 100%;
}
.button {
	color: white;
}
.button.orange {
	background: #ff8200;
}
.button.yellow {
	background: #f8b300;
}
.button.pink {
	background: #f57eb6;
}
.button.green {
	background: #64a70b;
}

.mask {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 100;
	background: rgba(0, 0, 0, .6);
}
.open-code-img {
	position: fixed;
	width: 100%;
	top: 100px;
	left: 0;
	padding: 0 10px;
	font-size: 14px;
	z-index: 101;
	box-sizing: border-box;
}
.open-code-img .row {
	position: relative;
	height: 40px;
	padding: 0 100px 0 105px;
	line-height: 40px;
	background: white;
}
.open-code-img .row text {
	position: absolute;
	top: 0;
	left: 30px;
}
.open-code-img .icon {
	position: absolute;
	top: 50%;
	left: 5px;
	width: 20px;
	height: 20px;
	margin-top: -10px;
}
.open-code-img input {
	display: inline-block;
	width: 100%;
	height: 40px;
	line-height: 40px;
	font-size: 14px;
}
.open-code-img .img {
	position: absolute;
	top: 50%;
	right: 0;
	width: 100px;
	height: 30px;
	margin-top: -15px;
}
.open-code-img .btn {
	display: block;
	width: 120px;
	height: 40px;
	margin: 30px auto 0;
	line-height: 40px;
	text-align: center;
	color: white;
	font-size: 16px;
	border: 0;
	border-radius: 5px;
	background: #f26a50;
}