page {
	min-height: 100%;
	font-size: 14px;
	background: #e9ebf0;
}
.orange {
	color: #ff8200;
}
.pink {
	color: #f57eb6
;
}
.yellow {
	color: #f8b300;
}
.green {
	color: #64a70b;
}
.container {
	padding: 10px;
}
.title {
	position: relative;
	display: block;
	padding-left: 10px;
	line-height: 40px;
	font-family: 微软雅黑;
	font-size: 16px;
    font-weight: normal;
    color: #6f6f6f;
}
.formGroup {
	margin-top: -1px;
	margin-bottom: 10px;
	border: 1px solid #d6d6d6;
	color: #333;
	background: white;
}
.formGroup .li {
	position: relative;
	height: 40px;
	padding: 0 20px 0 10px;
	line-height: 40px;
	border-bottom: 1px solid #d6d6d6;
}
.formGroup .select:after {
	content: '';
	position: absolute;
	top: 50%;
	right: 10px;
	width: 6px;
	height: 6px;
	border: solid #d6d6d6;
	border-width: 1px 1px 0 0;
	transform: translate(0, -50%) rotate(45deg);
}
.formGroup .last {
	border: 0;
}
.formGroup .select text:first-child {
	position: relative;
	padding-left: 30px;
}
.formGroup .select text:last-child {
	position: absolute;
	top: 0;
	right: 20px;
	max-width: 173px;
	margin-left: 105px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.formGroup .no-icon text:first-child {
	padding-left: 0;
}
.formGroup .pink {
	color: #f57eb6
;
}
.formGroup .orange {
	color: #ff8200;
}
.formGroup .green {
	color: #64a70b;
}
.formGroup .yellow {
	color: #f8b300;
}
.formGroup .yzCode {
	position: absolute;
	top: 50%;
	right: 20px;
	width: 100px;
	height: 30px;
	line-height: 30px;
	margin-top: -15px;
	border-radius: 5px;
	font-size: 12px;
	text-align: center;
	color: white;
}
.formGroup .yzCode.pink {
	background: #f57eb6
;
}
.formGroup .yzCode.orange {
	background: #ff8200;
}
.formGroup .yzCode.green {
	background: #64a70b;
}
.formGroup .yzCode.yellow {
	background: #f8b300;
}
.formGroup .input {
	padding: 0 20px 0 110px;
}
.formGroup .input text {
	position: absolute;
	top: 0;
	left: 10px;
	line-height: 40px;
}
.formGroup input {
	display: inline-block;
	width: 100%;
	height: 40px;
	line-height: 40px;
}
.formGroup .formGroup1 {
	border: 0;
}
.formGroup .check {
	position: absolute;
	top: 10px;
	right: 10px;
}
.formGroup .check view {
	display: inline-block;
	width: 40px;
	height: 25px;
	line-height: 25px;
	text-align: center;
	color: #333;
	font-size: 14px;
	border: 1px solid #d6d6d6;
	background: white;
}
.formGroup .check view:first-child {
	border-right: 0;
	border-radius: 5px 0 0 5px;
}
.formGroup .check view:last-child {
	border-left: 0;
	border-radius: 0 5px 5px 0;
}
.big-icon {
	width: 23px;
	height: 23px;
	padding-top: 8px;
}
.fr {
	float: right;
}
.fl {
	float: left;
}
.revamp {
	font-size: 16px;
}
.overflow {
	overflow: hidden;
}
.line {
	border-bottom: 1px solid #d6d6d6;
}
.formGroup .check .active {
	color: white;
}
.formGroup .check .active.orange {
	border-color: #ff8200;
	background: #ff8200;
}
.formGroup .check .active.pink {
	border-color: #f57eb6
;
	background: #f57eb6
;
}
.formGroup .check .active.green {
	border-color: #64a70b;
	background: #64a70b;
}
.formGroup .check .active.yellow {
	border-color: #f8b300;
	background: #f8b300;
}
